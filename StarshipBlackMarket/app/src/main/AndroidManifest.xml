<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cristopher.starshipblackmarket">

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/icon_appshipmarket"
        android:label="Starship Black Market"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MasterView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailView" />
        <activity android:name=".ReportView"></activity>
    </application>

</manifest>
